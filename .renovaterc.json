{
    "extends": ["config:base", "group:allNonMajor", "schedule:weekly"],
    "ignorePresets": [":prHourlyLimit2"],
    "rangeStrategy": "update-lockfile",
    "composer": {
        "enabled": false
    },
    "lockFileMaintenance": {
        "enabled": true
    },
    "cloneSubmodules": true,
    "packageRules": [
        {
          "matchPackageNames": ["@open-wc/scoped-elements"],
          "allowedVersions": "<3"
        }
    ]
}