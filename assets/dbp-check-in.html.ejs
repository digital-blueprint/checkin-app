                .int-link-internal{
                    transition: background-color 0.15s, color 0.15s;
                    border-bottom: var(--dbp-border-dark);
                }

                .int-link-internal:hover{
                    background-color: var(--dbp-override-hover-base);
                    color: var(--dbp-hover-text);
                }

                .int-link-internal:after{
